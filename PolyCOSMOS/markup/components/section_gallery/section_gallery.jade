                    li.gallery__item
                        img(src="%=static=%img/content/gallery/15.jpg" title="Фантом-манекен тканеэквивалентный ФМ-2").gallery__img
                    li.gallery__item
                        img(src="%=static=%img/content/gallery/16.jpg" title="Спускаемый аппарат космического корабля «Восток-6»").gallery__img
                    li.gallery__item
                        img(src="%=static=%img/content/gallery/17.jpg" title="Зал «Первые люди на орбите»").gallery__img
                    li.gallery__item
