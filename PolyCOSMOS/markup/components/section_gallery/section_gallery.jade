mixin section_gallery
    section.gallery
        .section__container.gallery__container
            .gallery__planet.gallery__planet--big
            .gallery__planet.gallery__planet--med
            .gallery__planet.gallery__planet--small
            .gallery__title
                img(src="%=static=%img/general/photo_title.png")
            .gallery__container
                ul.gallery__list
                    li.gallery__item
                        img(src="%=static=%img/content/gallery/1.jpg" title="Денис Кравченко (ОАО «Объединенная ракетно-космическая корпорация»), Екатерина Проничева (ВДНХ), Юлия Шахновская (Политехнический музей), Сергей Перов («РОСИЗО»)").gallery__img
                    li.gallery__item
                        img(src="%=static=%img/content/gallery/11.jpg" title="Мэр Москвы Сергей Собянин").gallery__img
                    li.gallery__item
                        img(src="%=static=%img/content/gallery/4.jpg" title="Министр культуры Российской Федерации Владимир Мединский").gallery__img
                    li.gallery__item
                        img(src="%=static=%img/content/gallery/3.jpg" title="Михаил Швыдкой").gallery__img
                    li.gallery__item
                        img(src="%=static=%img/content/gallery/5.jpg" title="Юлия Шахновская (Политехнический музей) и Иан Блэтчфорд (Science Museum, London)").gallery__img
                    li.gallery__item
                        img(src="%=static=%img/content/gallery/7.jpg" title="Скафандр программы «Марс-500», скафандр «Орлан-ДМА», скафандр лунный «Кречет»").gallery__img
                    li.gallery__item
                        img(src="%=static=%img/content/gallery/8.jpg" title="Спускаемый аппарат космического корабля «Союз ТМ-14»").gallery__img
                    li.gallery__item
                        img(src="%=static=%img/content/gallery/9.jpg" title="«Катапультируемая кабина животного с макетом собаки»").gallery__img
                    li.gallery__item
                        img(src="%=static=%img/content/gallery/10.jpg" title="Иан Блэтчфорд (Science Museum, London) и заместитель председателя правительства Российской Федерации Ольга Голодец").gallery__img
                    li.gallery__item
                        img(src="%=static=%img/content/gallery/12.jpg" title="Фотографии работ космического архитектора Галины Балашовой, cоздателя интерьеров космических кораблей").gallery__img
                    li.gallery__item
